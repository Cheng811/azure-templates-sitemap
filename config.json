{
    "repos": [
        {
            "name": "azure-sdk-for-net",
            "url": "https://github.com/azure/azure-sdk-for-net",
            "filters": [
                "sdk/.*/Sample.*\\.md",
                "!sdk/.*/README\\.md"
            ],
            "sitemap": "azure-sdk-for-net.xml"
        },
        {
            "name": "azure-sdk-for-java",
            "url": "https://github.com/azure/azure-sdk-for-java",
            "filters": [
                "sdk/.*/Sample.*\\.md",
                "!sdk/.*/README\\.md",
                "!sdk/.*/CHANGELOG\\.md"
            ],
            "sitemap": "azure-sdk-for-java.xml"
        },
        {
            "name": "azure-sdk-for-python",
            "url": "https://github.com/azure/azure-sdk-for-python",
            "filters": [
                "sdk/.*/samples/*\\.md",
                "sdk/.*/samples/*\\.py",
                "!sdk/.*/samples/README\\.md"
            ],
            "sitemap": "azure-sdk-for-python.xml"
        }
    ]
}
